{
    "name": "youtube/crud",
    "autoload": {
        "psr-4": {
            "Youtube\\Crud\\": "src/"
        }
    },
    "scripts": {
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "php -S 127.0.0.1:8000"
        ],
        "test:watch": [
            "Composer\\Config::disableProcessTimeout",
            "vendor/bin/phpunit-watcher watch tests"
        ]
    },
    "authors": [
        {
            "name": "Gustavo Souza",
            "email": "gustavosouzacco@gmail.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "spatie/phpunit-watcher": "^1.23"
    }
}
